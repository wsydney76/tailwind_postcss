{
  "name": "tailwind",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^10.0.2",
    "cssnano": "^4.1.10",
    "postcss": "^8.1.8",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "postcss-nested": "^5.0.1",
    "tailwindcss": "^2.0.1"
  },
  "scripts": {
    "dev": "postcss src/styles/main.css --output web/assets/styles/main.css",
    "watch": "postcss src/styles/main.css --output web/assets/styles/main.css --watch --verbose",
    "build": "postcss src/styles/main.css --output web/assets/styles/main.css --env production"
  }
}
